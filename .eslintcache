rdCorrectCount(data.wordCorrectCount);\n    setWordIncorrectCount(data.wordIncorrectCount);\n    setTotalScore(data.totalScore);\n    return setLoaded(true);\n  };\n\n  const handleSubmit = async (e) => {\n    e.preventDefault();\n    const token = localStorage.getItem('blogful-client-auth-token');\n    const res = await fetch(`${config.API_ENDPOINT}/language/guess`, {\n      method: 'POST',\n      headers: {\n        Accept: 'application/json',\n        'Content-Type': 'application/json',\n        Authorization: 'Bearer ' + token\n      },\n      body: JSON.stringify({ guess })\n    });\n    const data = await res.json();\n    setPrevWord(nextWord);\n    setNextWord(data.nextWord);\n    setCorrectAnswer(data.answer);\n    setWordCorrectCount(data.wordCorrectCount);\n    setWordIncorrectCount(data.wordIncorrectCount);\n    setTotalScore(data.totalScore);\n    return setIsCorrect(data.isCorrect);\n  };\n\n  const renderQuestion = () => {\n    return (\n      <>\n        <div>\n          <h2>Translate the word:</h2>\n          <span>{nextWord}</span>\n        </div>\n        <p>Your total score is: {totalScore}</p>\n        <p>You have answered this word correctly {wordCorrectCount} times.</p>\n        <p>\n          You have answered this word incorrectly {wordIncorrectCount} times.\n        </p>\n        <form onSubmit={handleSubmit}>\n          <label htmlFor='learn-guess-input'>\n            What's the translation for this word?\n          </label>\n          <input\n            type='text'\n            id='learn-guess-input'\n            required\n            onChange={(e) => setGuess(e.target.value)}\n          />\n          <button type='submit'>Submit your answer</button>\n        </form>\n      </>\n    );\n  };\n\n  const render = () => {\n    if (isCorrect === null) {\n      return renderQuestion();\n    } else if (isCorrect === false) {\n      return (\n        <IncorrectAnswer\n          totalScore={totalScore}\n          prevWord={prevWord}\n          correctAnswer={correctAnswer}\n          guess={guess}\n          nextQuestion={() => setIsCorrect(null)}\n        ></IncorrectAnswer>\n      );\n    } else if (isCorrect === true) {\n      return (\n        <CorrectAnswer\n          totalScore={totalScore}\n          prevWord={prevWord}\n          correctAnswer={correctAnswer}\n          guess={guess}\n          nextQuestion={() => setIsCorrect(null)}\n        ></CorrectAnswer>\n      );\n    }\n  };\n\n  return loaded && <main>{render()}</main>;\n};\n\nconst StyledForm = styled.form``;\n\nexport default LearningRoute;\n","/Users/dominicfaulring/code/thinkful/capstones/spaced-repetition-client/src/routes/DashboardRoute/DashboardRoute.js",[],"/Users/dominicfaulring/code/thinkful/capstones/spaced-repetition-client/src/config.js",[],"/Users/dominicfaulring/code/thinkful/capstones/spaced-repetition-client/src/components/RegistrationForm/RegistrationForm.js",[],"/Users/dominicfaulring/code/thinkful/capstones/spaced-repetition-client/src/components/LoginForm/LoginForm.js",[],"/Users/dominicfaulring/code/thinkful/capstones/spaced-repetition-client/src/components/IncorrectAnswer/IncorrectAnswer.js",[],"/Users/dominicfaulring/code/thinkful/capstones/spaced-repetition-client/src/components/CorrectAnswer/CorrectAnswer.js",[],"/Users/dominicfaulring/code/thinkful/capstones/spaced-repetition-client/src/components/Form/Form.js",[],"/Users/dominicfaulring/code/thinkful/capstones/spaced-repetition-client/src/components/Button/Button.js",[],{"ruleId":"95","replacedBy":"96"},{"ruleId":"97","replacedBy":"98"},{"ruleId":"99","severity":1,"message":"100","line":120,"column":7,"nodeType":"101","messageId":"102","endLine":120,"endColumn":17},"no-native-reassign",["103"],"no-negated-in-lhs",["104"],"no-unused-vars","'StyledForm' is assigned a value but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation"]